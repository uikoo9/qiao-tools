{
  "name": "qiao-cos",
  "version": "3.1.2",
  "description": "tencent cos upload tool on nodejs",
  "keywords": [
    "tencent",
    "cos",
    "upload",
    "tool",
    "nodejs"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-cos",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "bin",
    "src"
  ],
  "bin": {
    "qcos": "./bin/qcos.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-nodejs/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "cos-nodejs-sdk-v5": "^2.11.19",
    "qiao-cli": "^3.1.2",
    "qiao-file": "^3.1.2"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
