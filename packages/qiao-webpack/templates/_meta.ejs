<!-- important meta -->
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />

<!-- dns prefetch -->
<link rel="dns-prefetch" href="https://static.insistime.com/" />

<!-- 360 and baidu -->
<meta name="renderer" content="webkit" />
<meta http-equiv="Cache-Control" content="no-siteapp" />

<!-- for search-->
<meta name="keywords" content="{{ title || 'insistime.com' }}" />
<meta name="description" content="{{ title || 'insistime.com' }}" />
<meta name="author" contect="uikoo9@qq.com" />
