{
  "name": "qiao-cli",
  "version": "3.3.3",
  "description": "nodejs cli tool",
  "keywords": [
    "nodejs",
    "cli",
    "tool"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-cli",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-tools.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-tools/issues"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "inquirer": "^8.2.5",
    "progress": "^2.0.3"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
